    {
        "id": "5a6b27a5a350bdfb",
        "type": "ui_template",
        "z": "49ef9935a0084879",
        "group": "c9285b115cb483f1",
        "name": "Cadence",
        "order": 5,
        "width": 0,
        "height": 0,
        "format": "<div id=\"cadenceGaugeContainer\" style=\"width: 100%; max-width: 230px; margin: auto; position: relative;\">\n  <canvas id=\"cadenceGauge\"></canvas>\n  <canvas id=\"cadencePointer\" style=\"position: absolute; top: 0; left: 0;\"></canvas>\n</div>\n\n<script src=\"https://cdn.jsdelivr.net/npm/chart.js\"></script>\n\n<script>\n(function(scope) {\n  const gaugeCanvas = document.getElementById('cadenceGauge');\n  const pointerCanvas = document.getElementById('cadencePointer');\n  pointerCanvas.width = gaugeCanvas.width = 230;\n  pointerCanvas.height = gaugeCanvas.height = 230; // Full circle height\n\n  if (!scope.cadenceGaugeChart) {\n    const ctx = gaugeCanvas.getContext('2d');\n    scope.cadenceGaugeChart = new Chart(ctx, {\n      type: 'doughnut',\n      data: {\n        labels: ['', '', '', ''],\n        datasets: [{\n          data: [25, 40, 10, 25],\n          backgroundColor: ['#FF0000', '#00FF00', '#FFFF00', '#FF0000'],\n          borderWidth: 0\n        }]\n      },\n      options: {\n        rotation: 270,\n        circumference: 180,\n        cutout: '65%',\n        plugins: {\n          tooltip: { enabled: false },\n          legend: { display: false },\n          title: {\n            display: true,\n            text: 'Cadence (rpm)',\n            font: { size: 18 }\n          }\n        }\n      }\n    });\n\n    gaugeCanvas.insertAdjacentHTML('afterend', \n      '<div id=\"cadenceGaugeValue\" style=\"position: absolute; width: 100%; top: 90%; text-align: center; font-size: 16px; font-weight: bold;\"></div>'\n    );\n  }\n\n  function drawPointer(angle) {\n    const ctx = pointerCanvas.getContext('2d');\n    ctx.clearRect(0, 0, pointerCanvas.width, pointerCanvas.height);\n    ctx.strokeStyle = 'black';\n    ctx.lineWidth = 2;\n    \n    ctx.save();\n    // Translate to bottom center\n    ctx.translate(pointerCanvas.width / 2, pointerCanvas.height - 40);\n    ctx.rotate(angle);\n    \n    ctx.beginPath();\n    ctx.moveTo(0, 0);\n    ctx.lineTo(0, -100);\n    ctx.stroke();\n    \n    ctx.restore();\n  }\n\n  scope.$watch('msg.payload', function(cadence) {\n    if (typeof cadence === 'number') {\n      document.getElementById('cadenceGaugeValue').innerText = cadence + ' rpm';\n\n      let clamped = Math.max(0, Math.min(150, cadence));\n      let angle = (clamped / 150) * Math.PI; // 0 to PI\n      angle = angle - Math.PI / 2; // make 0 on left\n\n      drawPointer(angle);\n    }\n  });\n})(scope);\n</script>\n",
