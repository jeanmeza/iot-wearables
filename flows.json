        "width": 0,
        "height": 0,
        "format": "<div id=\"speedGaugeContainer\" style=\"width: 100%; max-width: 230px; margin: auto; position: relative;\">\n    <canvas id=\"speedGauge\"></canvas>\n    <canvas id=\"speedPointer\" style=\"position: absolute; top: 0; left: 0;\"></canvas>\n</div>\n\n<script src=\"https://cdn.jsdelivr.net/npm/chart.js\"></script>\n\n<script>\n    (function(scope) {\n  const gaugeCanvas = document.getElementById('speedGauge');\n  const pointerCanvas = document.getElementById('speedPointer');\n  pointerCanvas.width = gaugeCanvas.width = 230;\n  pointerCanvas.height = gaugeCanvas.height = 230;\n\n  if (!scope.speedGaugeChart) {\n    const ctx = gaugeCanvas.getContext('2d');\n    scope.speedGaugeChart = new Chart(ctx, {\n      type: 'doughnut',\n      data: {\n        labels: ['', '', '', ''],\n        datasets: [{\n          data: [10, 15, 15, 20], // 0-10, 10-25, 25-40, 40+\n          backgroundColor: ['#FF0000', '#FFFF00', '#00FF00', '#FF0000'],\n          borderWidth: 0\n        }]\n      },\n      options: {\n        rotation: 270,\n        circumference: 180,\n        cutout: '65%',\n        plugins: {\n          tooltip: { enabled: false },\n          legend: { display: false },\n          title: {\n            display: true,\n            text: 'Speed (km/h)',\n            font: { size: 18 }\n          }\n        }\n      }\n    });\n\n    gaugeCanvas.insertAdjacentHTML('afterend', \n      '<div id=\"speedGaugeValue\" style=\"position: absolute; width: 100%; top: 90%; text-align: center; font-size: 16px; font-weight: bold;\"></div>'\n    );\n  }\n\n  function drawPointer(angle) {\n    const ctx = pointerCanvas.getContext('2d');\n    ctx.clearRect(0, 0, pointerCanvas.width, pointerCanvas.height);\n    ctx.strokeStyle = 'black';\n    ctx.lineWidth = 2;\n    \n    ctx.save();\n    ctx.translate(pointerCanvas.width / 2, pointerCanvas.height - 40);\n    ctx.rotate(angle);\n    \n    ctx.beginPath();\n    ctx.moveTo(0, 0);\n    ctx.lineTo(0, -100);\n    ctx.stroke();\n    \n    ctx.restore();\n  }\n\n  scope.$watch('msg.payload', function(speed) {\n    if (typeof speed === 'number') {\n      document.getElementById('speedGaugeValue').innerText = speed.toFixed(1) + ' km/h';\n\n      let clamped = Math.max(0, Math.min(50, speed)); // cap at 50 km/h\n      let angle = (clamped / 50) * Math.PI; // 0 to PI\n      angle = angle - Math.PI / 2;\n\n      drawPointer(angle);\n    }\n  });\n})(scope);\n</script>",
